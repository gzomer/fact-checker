CREATE SCHEMA_CHANGE JOB add_structure FOR GRAPH FactChecking {
      ADD VERTEX Claim(PRIMARY_ID id UINT, Text STRING, Year INT, IsTrue BOOL) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true";
      ADD VERTEX Mention(PRIMARY_ID id UINT, Name STRING, EntityType STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true";
      ADD VERTEX Source(PRIMARY_ID id UINT, Name STRING, URL STRING) WITH STATS="OUTDEGREE_BY_EDGETYPE", PRIMARY_ID_AS_ATTRIBUTE="true";
      ADD DIRECTED EDGE mentions(FROM Claim, TO Mention) WITH REVERSE_EDGE="mention_claims";
      ADD DIRECTED EDGE sources(FROM Claim, TO Source) WITH REVERSE_EDGE="source_claims";
    }

